  "searches": {
    "license_searches": {
      "id": "license_searches",
      "name": "License Searches",
      "description": "Multi-state license searches for real estate brokers, salespersons, and bar licenses",
      "priority": "high",
      "data_folder": "research/license_searches/data",
      "file_patterns": {
        "state_findings": "research/license_searches/data/{state}/*_finding.json",
        "bar_findings": "research/license_searches/data/bar_licenses/*_bar_finding.json",
        "consolidated": "research/license_searches/data/consolidated/*.json"
      },
      "completion_criteria": {
        "required_folders": [
          "research/license_searches/data"
        ],
        "minimum_files": 200,
        "required_states": [
          "virginia", "maryland", "connecticut", "dc", "new_jersey", "new_york",
          "arizona", "florida", "georgia", "new_mexico", "north_carolina",
          "pennsylvania", "south_carolina", "utah"
        ],
        "file_pattern_checks": [
          "**/*_finding.json"
        ]
      },
      "subdirectories": {
        "by_state": true,
        "bar_licenses": true,
        "consolidated": true
      }
    },
    "company_registrations": {
      "id": "company_registrations",
